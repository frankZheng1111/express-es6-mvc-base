extend ../layouts/layout

block content
  .panel.panel-primary
    .panel-heading
      h1.panel-title Show Jade Base Feature:
    .panel-body
      h4.alert.alert-success(id=`test${demoId}`) Show valriables passed from routes(controller):  #{demo} - #{demoId}
      h4.alert.alert-warning show use components:
      include components/list.jade
      + list(['Show string;', 'And;', 'Hide Number;', 1, 2, 3])
    .panel-footer

  style.
    .panel {
      margin: 10%
    }
